{
  "name": "react-project-template",
  "version": "1.0.0",
  "description": "react-project-template",
  "main": "index.js",
  "scripts": {
    "dev": "tsc && NODE_ENV=development webpack-dev-server -d --hot --host 0.0.0.0 --config webpack.config.js --watch",
    "tsc": "tsc",
    "test": "jest",
    "production": "NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "deploy:staging": "aws s3 sync --acl public-read --profile atlas_ryan --delete dist/ s3://app-name-staging",
    "deploy:s3": "aws s3 sync --acl public-read --profile atlas_ryan --delete dist/ s3://app-name-production",
    "deploy:invalidate": "aws cloudfront --profile atlas_ryan create-invalidation --distribution-id ${CLOUDFRONT_ID} --paths '/*'",
    "lint": "eslint src/**/*.js src/**/*.jsx src/**/*.ts src/**/*.tsx"
  },
  "author": "Ryan Kim <oojikoo@gmail.com> (https://github.com/oojikoo)",
  "license": "UNLICENSED",
